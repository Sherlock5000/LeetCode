class Solution:
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        A = []
        if x < 0:
            return False
        while x != 0:
            a = x % 10
            x = int((x - a) / 10)
            A.append(a)
        if A == A[::-1]:
            return True
        else:
            return False
